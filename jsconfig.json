{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
  },

  // TypeScript
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  },
  "endOfLine": "tự động",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "types": ["vue-sweetalert2"],

}
